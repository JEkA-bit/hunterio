{
    "name": "messerli90/hunterio",
    "description": "Laravel wrapper to discover email addresses throgh the Hunter.io API",
    "keywords": [
        "messerli90",
        "hunterio",
        "email",
        "api",
        "wrapper"
    ],
    "homepage": "https://github.com/messerli90/hunterio",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Michael Messerli",
            "email": "michaelamesserli@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0|^7.0",
        "kitetail/zttp": "^0.6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Messerli90\\Hunterio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Messerli90\\Hunterio\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Messerli90\\Hunterio\\HunterioServiceProvider"
            ],
            "aliases": {
                "Hunterio": "Messerli90\\Hunterio\\HunterioFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
